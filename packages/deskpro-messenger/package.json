  "description": "DeskPRO embeddable messenger",
  "dependencies": {
    "@deskpro/portal-components": "^0.2.5",
    "axios": "^0.19.0-beta.1",
    "classnames": "^2.2.6",
    "contrast": "^1.0.1",
    "formik": "^1.1.1",
    "immer": "^1.7.3",
    "prop-types": "^15.6.2",
    "pusher-js": "^4.3.1",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.6.3",
    "react-frame-component": "^4.0.0",
    "react-froala-wysiwyg": "^2.9.0",
    "react-intl": "^2.7.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "socket.io-client": "^2.1.1"
  },
  "scripts": {
    "start": "npm-run-all -p watch:*",
    "build": "npm-run-all -s build:*",
    "watch:js": "react-scripts start",
    "build:js": "react-scripts build",
    "watch:css": "postcss -o public/assets/styles.css --verbose -m -w --env development styles/style.css",
    "build:css": "postcss --verbose --env production -m -o build/assets/styles.css styles/style.css",
    "postbuild": "node ./scripts/build-messenger.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "start-storybook": "npm-run-all -p watch:css storybook",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "jest-dom": "^2.1.1",
    "jest-extended": "^0.11.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.2",
    "postcss-autoreset": "^2.0.2",
    "postcss-cli": "^6.0.0",
    "postcss-extend-rule": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-reporter": "^6.0.0",
    "react-testing-library": "^5.3.0",
    "stylelint": "^9.5.0",
    "stylelint-config-suitcss": "^14.0.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "uglify-js": "^3.4.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/stories/**",
      "!**/node_modules/**"
    ]
  }
}